<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.heren.turtle.server.dao.hisDao.HisDiagnosisDao">

    <resultMap id="diagnosisMap" type="java.util.Map">
        <result column="series" property="req_no"/>
        <result column="patient_id" property="patient_id"/>
        <result column="visit_id" property="in_hos_sum"/>
        <result column="DEPT_CODE" property="phase"/>
        <result column="PLAN_TIME" property="diag_id"/>
        <result column="RECORD_TIME" property="diag_name"/>
        <result column="VITALSIGN_NAME" property="outhostext"/>
        <result column="UPDATE_TIME" property="icd_code"/>
        <result column="ACTION_TYPE" property="diag_time"/>
        <result column="ACTION_TYPE" property="diag_doc"/>
        <result column="ACTION_TYPE" property="diag_docno"/>
